{
    "_version": "1.12.0",
    "sap.app": {
        "id": "sap.ui.cliente",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "sap.ui.cliente.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.93",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sap.ui.cliente.i18n.i18n",
                    "supportedLocales": [
                        ""
                    ],
                    "fallbackLocale": ""
                }
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "type": "View",
                "viewType": "XML",
                "path": "sap.ui.cliente.view",
                "controlId": "app",
                "controlAggregation": "pages"
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "ListaClientes",
                    "target": "ListaClientes"
                },
                {
                    "pattern": "detalhes/{id}",
                    "name": "detalhes",
                    "target": "detalhes"
                }
            ],
            "targets": {
                "ListaClientes": {
                    "id": "ListaClientes",
                    "name": "ListaClientes"
                },
                "detalhes": {
                    "id": "detalhes",
                    "name": "Detalhes"
                }
            }
        }
    }
}